<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E6DC4439"
    tools:context=".DashboardFragment">

    <Button
        android:id="@+id/soundsButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Sounds"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calcButtonDashboard" />

    <Button
        android:id="@+id/calcButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Calculator"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/characterButtonDashboard" />

    <Button
        android:id="@+id/librariesButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Libraries"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/diceButtonDashboard" />

    <Button
        android:id="@+id/dungeonAIButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="10dp"
        android:text="AI Dungeon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userLabel" />

    <Button
        android:id="@+id/diceButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Dice"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notesButtonDashboard" />

    <Button
        android:id="@+id/characterButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Characters"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gameSetupButtonDashboard" />

    <Button
        android:id="@+id/notesButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Notes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gameSetupButtonDashboard" />

    <Button
        android:id="@+id/gameSetupButtonDashboard"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="10dp"
        android:text="Game Setup"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userLabel" />

    <Button
        android:id="@+id/faqButtonDashboard"
        android:layout_width="150dp"
        android:layout_height="54dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="16dp"
        android:text="FAQ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/settingsButtonDashboard"
        android:layout_width="150dp"
        android:layout_height="54dp"
        android:layout_marginEnd="8dp"
        android:text="Grid"
        app:layout_constraintBottom_toTopOf="@+id/contactButtonDashboard"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/logoutButtonDashboard"
        android:layout_width="150dp"
        android:layout_height="54dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="Logout"
        app:layout_constraintBottom_toTopOf="@+id/faqButtonDashboard"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/tutorialButtonDashboard"
        android:layout_width="169dp"
        android:layout_height="51dp"
        android:layout_marginBottom="10dp"
        android:text="Tutorial"
        app:layout_constraintBottom_toTopOf="@+id/logoutButtonDashboard"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/contactButtonDashboard"
        android:layout_width="150dp"
        android:layout_height="54dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="16dp"
        android:text="Contact Us"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="212dp"
        android:layout_height="142dp"
        android:layout_marginTop="16dp"
        android:background="#000000"
        android:padding="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:srcCompat="@drawable/build_your_own_adventure_2" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="308dp"
        android:layout_height="26dp"
        android:layout_marginTop="4dp"
        android:text="Build Your Own Adventure"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/userLabel"
        android:layout_width="293dp"
        android:layout_height="32dp"
        android:layout_marginTop="6dp"
        android:text="Welcome User Label"
        android:textAlignment="center"
        android:textColor="#681111"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

</androidx.constraintlayout.widget.ConstraintLayout>